zodb-db-type mysql
zodb-host <%= node['RM']['ZODBHOST'] %>
zodb-port <%= node['RM']['ZODBPORT'] %>
zodb-db <%= node['RM']['ZODBDB'] %>
zodb-user <%= node['RM']['ZODBUSER'] %>
zodb-password <%= node['RM']['ZODBPASSWORD'] %>
zodb-admin-user <%= node['RM']['ZODBADMINUSER'] %>
zodb-admin-password <%= node['RM']['ZODBADMINPASSWORD'] %>
zodb-cacheservers 127.0.0.1:11211
zodb-cachesize 1000

amqphost localhost
amqpport 5672
amqpvhost /zenoss
amqpuser zenoss
amqppassword zenoss
amqpusessl 0
amqpadminport 55672
amqpadminusessl 0

zep-db-type mysql
zep-host <%= node['RM']['ZEPHOST'] %>
zep-port <%= node['RM']['ZEPPORT'] %>
zep-db <%= node['RM']['ZEPDB'] %>
zep-user <%= node['RM']['ZEPUSER'] %>
zep-password <%= node['RM']['ZEPPASSWORD'] %>
zep-admin-user <%= node['RM']['ZEPADMINUSER'] %>
zep-admin-password <%= node['RM']['ZEPADMINPASSWORD'] %>
zep-uri http://localhost:8084
